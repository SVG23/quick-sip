  "dependencies": {
    "browserify": "^16.5.0",
    "color-log": "0.0.2",
    "del": "^5.1.0",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^7.0.1",
    "gulp-concat": "^2.6.1",
    "gulp-jshint": "^2.1.0",
    "gulp-load-plugins": "^2.0.1",
    "gulp-sass": "^4.0.2",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-tap": "^2.0.0",
    "gulp-uglify": "^3.0.2",
    "jasmine": "^3.5.0",
    "jshint": "^2.10.2",
    "jshint-stylish": "^2.2.1",
    "lodash": "^4.17.15",
    "minimist": "^1.2.0",
    "through2": "^3.0.1",
    "vinyl-buffer": "^1.0.1",
    "vinyl-source-stream": "^2.0.0",
    "watchify": "^3.11.1"
  },
  "engines": {
    "node": ">=0.8.0"
  },
  "scripts": {
    "test": "jasmine",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect",
    "release": "release-it"
  },
  "snyk": true,
  "devDependencies": {
    "release-it": "^12.3.5",
    "snyk": "^1.230.5"
  },
  "release-it": {
    "github": {
      "release": true,
      "releaseName": "Release v${version}"
